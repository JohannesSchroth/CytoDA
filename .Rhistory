decode_caesar <- function(txt = txt, shift) {
encr <- txt %>%
strsplit('') %>%
unlist %>%
tolower()
punct <- grep('[[:punct:]]', encr)
a <- match(encr, letters) - shift
a[which(a == 0)] <- a[which(a == 0)] - 1
a[which(a <= 0)] <- a[which(a <= 0)] + 26
a[which(a > 26)] <- a[which(a > 26)] - 26
b <- replace_na(letters[a], ' ')
b[punct] <- encr[punct]
paste(b, collapse = '')
}
decode_caesar(shift = -5)
